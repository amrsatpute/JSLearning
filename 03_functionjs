//function with no agrument with no value
function showFullName(){
console.log("My name is Amar Satpute:");
}
showFullName();// functon call or invoke

//function with no agrument with no value
function showAge(age){
    console.log("My age is:", age);
}
showAge(27);
// function with no arguments and return value
function fulName(){
    var name = "Amar Satpute";
    return name;
}
var fName = fName;
console.log(fName);

// function with argument and return value

function sum0Number(){
    var sum = num1 + num2 + num3;
    return sum;
}




